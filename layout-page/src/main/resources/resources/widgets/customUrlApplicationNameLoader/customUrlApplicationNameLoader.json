{"designerVersion":"1.0.0-SNAPSHOT","id":"customUrlApplicationNameLoader","name":"urlApplicationNameLoader","lastUpdate":1427960878488,"template":"","controller":"function($scope, $location) {\n    var urlAsTab = $location.absUrl().split(\"/\");\n    $scope.parameters.applicationName = urlAsTab[urlAsTab.length-2];\n    $scope.parameters.pageToken = urlAsTab[urlAsTab.length-1];\n}","custom":true,"parameters":[{"label":"application name","name":"applicationName","type":"text","defaultValue":"This is the initial value","bidirectional":false},{"label":"Page token","name":"pageToken","type":"text","bidirectional":false}]}